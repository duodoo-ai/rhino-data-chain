<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="1">
        <!--付款申请 序列号-->
        <record id="seq_advance_payment_apply_a" model="ir.sequence">
            <field name="name">付款申请单号</field>
            <field name="code">advance.payment.apply.a</field>
            <field name="prefix">APA%(year)s%(month)s%(day)s-</field>
            <field name="padding">3</field>
            <field name="is_renumber">True</field>
        </record>

        <record id="seq_advance_payment_apply_b" model="ir.sequence">
            <field name="name">付款申请单号</field>
            <field name="code">advance.payment.apply.b</field>
            <field name="prefix">APB%(year)s%(month)s%(day)s-</field>
            <field name="padding">3</field>
            <field name="is_renumber">True</field>
        </record>

        <!--收款申请 序列号-->
        <record id="seq_advance_receive_apply_a" model="ir.sequence">
            <field name="name">收款申请单号</field>
            <field name="code">advance.receive.apply.a</field>
            <field name="prefix">ARA%(year)s%(month)s%(day)s-</field>
            <field name="padding">3</field>
            <field name="is_renumber">True</field>
        </record>
        <record id="seq_advance_receive_apply_b" model="ir.sequence">
            <field name="name">收款申请单号</field>
            <field name="code">advance.receive.apply.b</field>
            <field name="prefix">ARB%(year)s%(month)s%(day)s-</field>
            <field name="padding">3</field>
            <field name="is_renumber">True</field>
        </record>

    </data>
</odoo>